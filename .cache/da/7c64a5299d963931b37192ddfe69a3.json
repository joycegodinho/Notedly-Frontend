ce":"pages/signup.js","original":{"line":32,"column":0}},{"name":"SignUp","generated":{"line":114,"column":15},"source":"pages/signup.js","original":{"line":68,"column":15}}],"sources":{"pages/signup.js":"import React, { useEffect, useState } from 'react';\r\nimport styled from 'styled-components';\r\nimport { useMutation, useApolloClient, gql } from '@apollo/client'\r\n\r\nimport Button from '../components/Button';\r\n\r\nconst SIGNUP_USER = gql`\r\n    mutation signUp($email: String!, $username: String!, $password: String!){\r\n        signUp(email: $email, username: $username, password: $password)\r\n    }\r\n`;\r\n\r\nconst Wrapper = styled.div`\r\n    border 1px solid #f5f4f0;\r\n    max-width: 500px;\r\n    padding: 1em;\r\n    margin: 0 auto;\r\n`;\r\n\r\nconst Form = styled.form`\r\n    label,\r\n    input {\r\n        display: block;\r\n        line-height: 2em;\r\n    }\r\n    input {\r\n        width: 100%;\r\n        margin-bottom: 1em;\r\n    }\r\n`;\r\n\r\nconst SignUp = props => {\r\n\r\n    const [values, setValues] = useState();\r\n    const onChange = event => {\r\n        setValues({...values, [event.target.name]:event.target.value});\r\n    };\r\n\r\n    useEffect (() => { document.title = 'Sign Up - Notedly' });\r\n\r\n    const [signUp, { loading, error }] = useMutation(SIGNUP_USER, {\r\n        onCompleted: data => console.log(data.signUp)\r\n    });\r\n\r\n    return (\r\n        <Wrapper>\r\n            <h2>Sign Up</h2>\r\n            <Form onSubmit={event => {\r\n                event.preventDefault();\r\n                signUp({variables: {...values}})\r\n            }}>\r\n\r\n                <label htmlFor=\"username\">Username:</label>\r\n                <input required type=\"text\" id=\"username\" name=\"username\" placeholder=\"username\" onChange={onChange}/>\r\n                \r\n                <label htmlFor=\"email\">Email:</label>\r\n                <input required type=\"email\" id=\"email\" name=\"email\" placeholder=\"Email\" onChange={onChange}/>\r\n                \r\n                <label htmlFor=\"password\">Password:</label>\r\n                <input required type=\"password\" id=\"password\" name=\"password\" placeholder=\"Password\" onChange={onChange}/>\r\n\r\n                <Button type=\"submit\">Submit</Button>\r\n            </Form>\r\n        </Wrapper>\r\n    );\r\n};\r\n\r\nexport default SignUp;"},"lineCount":null}},"error":null,"hash":"94a0b0d8c6d128f18f64ceba63673c7e","cacheData":{"env":{}}}